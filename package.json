{
  "name": "codytolene-github-io",
  "version": "2.0.0",
  "description": "Cody Tolene's Personal Angular Website",
  "author": "Cody Tolene",
  "license": "AGPL-3.0",
  "homepage": "https://www.codytolene.com",
  "private": true,
  "keywords": [
    "Cody",
    "Tolene",
    "Personal",
    "Website"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CodyTolene/codytolene.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/CodyTolene/codytolene.github.io/issues"
  },
  "contributors": [
    "Cody Tolene (https://www.codytolene.com)"
  ],
  "engines": {
    "node": ">=16.14.12 <17",
    "npm": ">=8.5.0"
  },
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "lint": "ng lint",
    "ng": "ng",
    "package-updates:check": "npx npm-check-updates",
    "package-updates:apply": "npx npm-check-updates --upgrade",
    "postinstall": "is-ci || husky install",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "start": "ng serve --open",
    "start:prod": "ng serve --configuration production --open",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "~13.3.6",
    "@angular/common": "~13.3.6",
    "@angular/compiler": "~13.3.6",
    "@angular/core": "~13.3.6",
    "@angular/forms": "~13.3.6",
    "@angular/platform-browser": "~13.3.6",
    "@angular/platform-browser-dynamic": "~13.3.6",
    "@angular/router": "~13.3.6",
    "particles.js": "^2.0.0",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.6",
    "@types/jasmine": "~4.0.3",
    "@types/node": "17.0.32",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.2.9",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jasmine-core": "~4.1.1",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "npm-check-updates": "^12.5.11",
    "prettier": "^2.6.2",
    "typescript": "~4.6.4"
  }
