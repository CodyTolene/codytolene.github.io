{
  "name": "codytolene",
  "version": "21.0.1",
  "scripts": {
    "build": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:sitemap": "ts-node --project .scripts/tsconfig.json .scripts/generate-sitemap.ts",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ng": "ng",
    "postinstall": "is-ci || husky install",
    "prepare": "husky install",
    "serve:ssr:codytolene": "node dist/codytolene/server/server.mjs",
    "start": "ng serve --open",
    "test": "ng test",
    "update:check": "ncu",
    "update:run": "ncu -u",
    "watch": "ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "21.1.5",
    "@angular/cdk": "^21.1.5",
    "@angular/common": "21.1.5",
    "@angular/compiler": "21.1.5",
    "@angular/core": "21.1.5",
    "@angular/fire": "^21.0.0-rc.0",
    "@angular/forms": "21.1.5",
    "@angular/material": "^21.1.5",
    "@angular/material-luxon-adapter": "^21.1.5",
    "@angular/platform-browser": "21.1.5",
    "@angular/platform-browser-dynamic": "21.1.5",
    "@angular/platform-server": "^21.1.5",
    "@angular/router": "21.1.5",
    "@angular/service-worker": "^21.1.5",
    "@angular/ssr": "^21.1.4",
    "@ngneat/until-destroy": "^10.0.0",
    "@proangular/ngx-gist": "^21.0.0",
    "@proangular/ngx-scroll-top": "^21.0.0",
    "@proangular/pro-form": "^21.0.0",
    "@proangular/pro-table": "^21.0.0",
    "express": "^5.2.1",
    "io-ts": "^2.2.22",
    "luxon": "^3.7.2",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^21.1.4",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.5",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/express": "^5.0.6",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "angular-eslint": "^21.2.0",
    "eslint": "9.0.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jsdom": "^28.1.0",
    "npm-check-updates": "^19.4.1",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.14.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": "24"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.10.0"
}
