{
  "name": "codytolene",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ng": "ng",
    "postinstall": "is-ci || husky install",
    "prepare": "husky install",
    "serve:ssr:codytolene": "node dist/codytolene/server/server.mjs",
    "start": "ng serve --open",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/cdk": "^21.0.5",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/fire": "^21.0.0-rc.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.5",
    "@angular/material-luxon-adapter": "^21.0.5",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/ssr": "^21.0.4",
    "@proangular/ngx-gist": "^21.0.0",
    "@proangular/ngx-scroll-top": "^21.0.0",
    "@proangular/pro-form": "^21.0.0",
    "@proangular/pro-table": "^21.0.0",
    "express": "^5.1.0",
    "io-ts": "^2.2.22",
    "luxon": "^3.7.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/express": "^5.0.1",
    "@types/luxon": "^3.7.1",
    "@types/node": "^20.17.19",
    "angular-eslint": "^21.1.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.7.4",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.8"
  }
}
